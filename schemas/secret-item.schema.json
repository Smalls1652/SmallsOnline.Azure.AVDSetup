{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Secret Item",
    "description": "An item for a SecretStore item.",
    "type": "object",
    "properties": {
        "secretVaultName": {
            "description": "The name of the SecretManagement vault.",
            "type": "string"
        },
        "secretName": {
            "description": "The name of the secret.",
            "type": "string"
        },
        "secretType": {
            "description": "The type of secret.",
            "type": "string"
        }
    },
    "required": [
        "secretVaultName",
        "secretName",
        "secretType"
    ]
}